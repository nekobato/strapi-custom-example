{
  "kind": "collectionType",
  "collectionName": "owned_medias",
  "info": {
    "singularName": "owned-media",
    "pluralName": "owned-medias",
    "displayName": "OwnedMedia"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "slug": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "owned_media_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::owned-media-profile.owned-media-profile"
    },
    "owned_media_profiles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::owned-media-profile.owned-media-profile"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "banner_text": {
      "type": "string"
    },
    "introduction": {
      "type": "customField",
      "customField": "plugin::lexical.lexical",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "content": {
      "type": "customField",
      "customField": "plugin::lexical.lexical",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "published_datetime": {
      "type": "datetime",
      "required": true
    }
  }
}
